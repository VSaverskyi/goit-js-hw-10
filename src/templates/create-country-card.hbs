{{#each this}}
<div class="flags-wrapper">
    <img src={{this.flags.svg}} alt={{this.flags.alt}} width="40">
    <h2 class="country-name">{{this.name.official}}</h2>
</div>
<ul class="country-info list">
    <li class="country-info-item">
        <p class="capital">Capital: {{this.capital}}</p>
    </li>
    <li class="country-info-item">
        <p class="population">Population: {{this.population}}</p>
    </li>
    <li class="country-info-item">
        <p class="languages">Languages: {{Object.values(this.languages)}}</p>
    </li>
</ul>
{{/each}}